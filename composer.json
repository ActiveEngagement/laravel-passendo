{
    "name": "actengage/laravel-passendo",
    "description": "A Passendo package for Laravel.",
    "type": "library",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0|~7.0",
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\LaravelPassendo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Actengage\\LaravelPassendo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Actengage\\LaravelPassendo\\PassendoServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
